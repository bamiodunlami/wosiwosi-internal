
<%- include('include/header') -%>


<!-- show page after reading the js -->

    <section class="order-page">
        <div class="container">

            <h6 class="alert alert-success text-center">Completed Order</h6>

            <h6 class="alert alert-warning text-center">Completed: <%=order.length%></h6>
            <!-- <div class="row d-flex justify-content-center alert alert-light">
                <div class="col-md-6 dateFilter">
                        <form action="/order" method="get" class="row">
                            <div class="dateFilter-item col-sm-12">
                                <input type="text" placeholder="Search Order..." class="form-control" name="fromDate" value="">
                            </div>
                        </form>
                </div>
            </div> -->
            <!-- table -->
            <div class="row order-table">
                <div class="col-12">
                    <table class="table table-striped table-hover ">
                        <thead>
                        <tr>
                            <th scope="col">Order</th>
                            <th scope="col">Meat Picker</th>
                            <th scope="col">Dry Picker</th>
                            <th scope="col">Packer</th>
                        </tr>
                        </thead>
                        <tbody class="">
                            <% for(i =0; i<order.length; i++ ){%>
                                <tr class="eachOrder">
                                    <td><a href="/single-order-processing?id=<%=order[i].orderNumber%>"><%=order[i].orderNumber%></a></td>
                                        <td><%=order[i].meatPicker.fname%></td>
                                        <td><%=order[i].dryPicker.fname%></td>
                                        <td><%=order[i].packer.fname%></td>
                                </tr>
                            <%}%>
                        </tbody>
                    </table>
                </div>
            </div>
        
            <!-- back to dashboard -->
            <div class="row">
                <div class="text-center">
                    <% if(user.role == "admin"){%>
                        <a class="btn btn-success btn-md" href="/admin/online">Back to dashboard</a>
                   <% }else{%>
                        <a class="btn btn-success btn-md" href="/staff/online">Back to dashboard</a>
                    <%}%>

                </div>
            </div>

        </div>
    </section>


<%- include('include/footer') -%>
