<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Orders</title>
        <meta name="description" content="Wosiwosi Picking and Packer">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- Stylesheet -->


        <!-- Bostrap stylesheet -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
        
        <!-- Google Fonts  -->
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@200;300;600&display=swap" rel="stylesheet">
        <!--internal style-->
        <link href="css/style.css" rel="stylesheet">
    </head>
    <body>
        
        <!-- sor by number or order -->
        <section class="order">
            <h4 class="col-sm-12 text-center">SHOWING <span class="orderUnit" id="orderUnit">?</span> ORDERS</h4>
           
            <div class="row sort text-center">

                <!-- <div class="col-md-4 col-sm-12">
                    <input type="number" id="filter" class="amountToView" name="viewAmount" value="0" max="100" min="20">
                     <button class="btn btn-sm btn-warning" id="btn-filter">View</button>
                </div>  -->

                <div class=" col-sm-12 text-center">
                    <input type="text" id="search-filter" class="amountToView" name="viewAmount" placeholder="Search Order">
                </div>
            </div>


            <!-- Sort by date -->
            <!-- <div class="row sort">
                <div class="col-sm-12">
                   From: <input type="date" id="fromDate" class="fromDate amountToView">
                   to: <input type="date" id="toDate" class="toDate amountToView">
                   <button id="sort-by-date" class="btn btn-warning btn-sm">Find</button>
                </div>
            </div> -->

            <!-- user table details -->
            <div class="row picker-details">
                <div class="col-6 text-center picker-name">
                    <h4 id="staffRole"><%=logOffice%>: <span id="username" class="username-name"><%=logUser%></span></h4>
                </div>

                <div class="col-6 text-center picker-name">
                    <h4>Today: <span id="logDate" class="username-name"><%=logDate%></span></h4>
                </div> 

            </div>

            <div class="row" id="tableRow">
                <table id="extable" class="table table-hover">
                    <tr>
                    <th>Order No.</th>
                    <th>Customer Name</th>
                    <th>Date</th>
                    <th>Cu</th>
                    <th>Pi</th>
                    <th>Pa</th>
                    </tr>



                    <tbody id="myTable">

                    <!-- loading effect -->
                    <div class="text-center loading-hide" id="loading">
                        <div class="spinner-border text-secondary" role="status">
                        </div>
                    </div>
                    <!-- loading effect end -->


                    </tbody>
                  </table>
            </div>
            <button id="logout" type="submit" class="btn btn-danger btn-lg">logout</button>
        </section>


           
        <!--Boostrap-->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
       <!--jquery-->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
        <!--internal js-->
        <script src="js/call.js"></script>

    </body>
</html>